-resourceonly: true
-include: ${.}/buildpath.bnd	
mavendeps: ${sjoin;\n; ${template;-maven-dependencies;${@}}}

-includeresource: resources,\
	{template/geckoEMFModels.maven=resources/template/geckoEMFModels.maven}	
	
Provide-Capability: \
	${gecko-base}

gecko-base:\
    bnd.library;\
        bnd.library     = geckoEMFModels; \
        path            = template
        
Bundle-Name: Gecko EMF Model Workspace Library
Bundle-Description: Workspace library for dependencies to all models